{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "description": "A linderlabs registry of components, hooks, pages, etc.",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "homepage": {
        "type": "string"
      },
      "labels": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          }
        }
      },
      "items": {
        "type": "array",
        "items": {
          "$ref": "https://linderlabsai.vercel.app/schema/registry-item.json"
        }
      }
    },
    "required": ["name", "homepage", "items"],
    "uniqueItems": true,
    "minItems": 1
  }